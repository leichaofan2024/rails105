<h1>题库首页</h1>
<table class="table">
  <thead>
    <th>试卷编号</th>
    <th>试卷标题</th>
    <th>试卷描述</th>
  </thead>
  <tbody>
    <% @quizzes.each do |quiz| %>
      <tr>
        <td><%= quiz.id %></td>
        <td> <%= link_to quiz.title, quiz_path(quiz) %></td>
        <td><%= quiz.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
